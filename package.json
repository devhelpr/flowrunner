  "license": "MIT",
  "dependencies": {
    "@reactivex/rxjs": "^6.4.0",
    "@types/uuid": "^3.4.4",
    "moment": "^2.24.0",
    "promise": "^8.0.2",
    "rxjs": "^6.4.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.21.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/**/*"
