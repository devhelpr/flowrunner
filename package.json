{
  "name": "@devhelpr/flowrunner",
  "version": "0.18.1",
  "description": "Library which runs flows",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/flowrunner.esm.js",
  "size-limit": [
    {
      "path": "dist/flowrunner.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/flowrunner.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "repository": "https://www.github.com/devhelpr/flowrunner",
  "author": "Maikel van de Lisdonk",
  "license": "MIT",
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "moment": "^2.29.4",
    "promise": "^8.3.0",
    "rxjs": "^6.4.0",
    "uuid": "^9.0.0",
    "@devhelpr/expressionrunner": "^0.3.1"
  }
}
