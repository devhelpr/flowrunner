{
  "name": "@devhelpr/flowrunner",
  "version": "0.10.26",
  "description": "Library which runs flows",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --no-cache --config jestconfig.json",
